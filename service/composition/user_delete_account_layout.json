{
  "client": {
    "config": {
      "title": "User settings",
      "templates": {
        "layout": {
          "to": "._container",
          "html": "/user/user_delete_account.html"
        }
      },
      "defaultTemplate": "layout",
      "states": {
        "showDeleteAccount": [
          {
            "sel": ".delete-account",
            "rm": [
              "hide"
            ]
          }
        ],
        "hideDeleteAccount": [
          {
            "sel": ".delete-account",
            "add": [
              "hide"
            ]
          }
        ]
      },
      "domEvents": [
        {
          "on": "click",
          "element": "deleteAccount",
          "flow": "showDeleteAccount"
        },
        {
          "on": "click",
          "element": "deleteThisAccountCancel",
          "flow": "hideDeleteAccount"
        }
      ]
    },
    "markup": [
      "/user/user_delete_account.html"
    ],
    "flow": [
      [
        "renderedDOM",
        [
          ":ALTR",
          {
            "data": {
              "clear": false
            }
          }
        ],
        "nav_user_settings_layout/render"
      ],
      [
          "renderedDOM",
          [":private_layout/state", "showFooter"],
          [":private_layout/state", "displayContainer"]
      ],
      [
        "showDeleteAccount",
        [":state", "showDeleteAccount"]
      ],
      [
        "hideDeleteAccount",
        [":state", "hideDeleteAccount"]
      ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "user_delete_account_layout"
}
