{
  "client": {
    "config": {
      "templates": {
        "layout": {
          "to": ".main-nav",
          "html": "/root/private/nav_main_private.html",
          "render": true
        }
      },
      "defaultTemplate": "layout",
      "states": {
          "resetNav": [
              {
                  "sel": ".active-visible li",
                  "add": [
                      "hide"
                  ]
              }
          ],
          "nav": [
              {
                  "states": [
                      "resetNav"
                  ],
                  "rm": [
                      "hide"
                  ]
              }
          ],
          "showNavigationMenu": [
              {
                  "sel": ".navigation-menu-hidden",
                  "rm": ["hide"]
              },
              {
                  "sel": ".active-visible",
                  "add": ["hide"]
              }
          ],
          "hideNavigationMenu": [
              {
                  "sel": ".navigation-menu-hidden",
                  "add": ["hide"]
              },
              {
                  "sel": ".active-visible",
                  "rm": ["hide"]
              }
          ],
          "toggleDirection": [
              {
                  "sel": ".direction-toggle",
                  "toggle": [
                      "hide"
                  ]
              },
              {
                  "sel": ".direction-arrow",
                  "toggle": [
                      "hide"
                  ]
              }
          ],
          "hideDirection": [
              {
                  "sel": ".direction-toggle",
                  "add": [
                      "hide"
                  ]
              },
              {
                  "sel": ".direction-arrow",
                  "add": [
                      "hide"
                  ]
              }
          ],
          "toggleMobileMenu": [{
              "sel": ".first-col",
              "toggle": ["show"]
          },
          {
              "sel": ".collapsable-nav-1",
              "toggle": ["show"]
          },
          {
              "sel": ".second-col",
              "toggle": ["show"]
          },
          {
              "sel": ".collapsable-nav-2",
              "toggle": ["show"]
          }
          ]
      },
      "domEvents": [
        {
          "on": "mouseover",
          "selector": ".navigation-menu",
          "flow": "showNavigationMenu"
        },
        {
            "on": "mouseleave",
            "selector": ".navigation-menu",
            "flow": "hideNavigationMenu"
        },
        {
            "on": "click",
            "element": "home",
            "flow": "privateHome"
        },
        {
          "on": "click",
          "element": "apps",
          "flow": "navApps"
        },
        {
          "on": "click",
          "element": "stores",
          "flow": "navStores"
        },
        {
          "on": "click",
          "element": "domains",
          "flow": "navDomains"
        },
        {
          "on": "click",
          "element": "new",
          "flow": "navNew"
        },
        {
          "on": "click",
          "element": "projects",
          "flow": "navProjects"
        },
        {
          "on": "click",
          "element": "organizations",
          "flow": "navOrganizations"
        },
        {
          "on": "click",
          "element": "billings",
          "flow": "navBillings"
        },
        {
          "on": "click",
          "element": "versions",
          "flow": "navVersions"
        },
        {
          "on": "click",
          "element": "account-settings",
          "flow": "accountSettings"
        },
        {
          "on": "click",
          "element": "profile-settings",
          "flow": "profileSettings"
        },
        {
          "on": "click",
          "element": "user",
          "flow": "navUserProfile"
        },
        {
          "on": "click",
          "element": "dropdown",
          "flow": "toggleShortcuts"
        },
        {
          "on": "click",
          "selector": ".direction-toggle",
          "flow": "hideShortcuts"
        },
        {
          "on": "mouseleave",
          "selector": ".direction-toggle",
          "flow": "hideShortcuts"
        },
        {
          "on": "click",
          "element": "signout",
          "flow": "navSignout"
        },
        {
          "on": "click",
          "element": "blog",
          "flow": "navBlog"
        },
        {
          "on": "click",
          "element": "docs",
          "flow": "navDocs"
        },
        {
          "on": "click",
          "element": "support",
          "flow": "navHelp"
        },
        {
            "on": "click",
            "selector": ".menu-icon",
            "flow": "toggleMobileMenu"
        },
        {
          "on": "click",
          "element": "jillix-logo",
          "flow": "privateHome"
        }
      ]
    },
    "flow": [
      [
          "privateHome",
          [":private_router/route", "/"]
      ],
      [
          "showNavigationMenu",
          [":state", "showNavigationMenu"]
      ],
      [
          "hideNavigationMenu",
          [":state", "hideNavigationMenu"]
      ],
      [
          "navApps",
          [":private_router/route", "/apps"],
          [":state", "showApps"],
          [":state", "hideNavigationMenu"]
      ],
      [
          "navStores",
          [":private_router/route", "/stores"]
      ],
      [
          "navDomains",
          [":private_router/route", "/domains"]
      ],
      [
          "navNew",
          [":private_router/route", "/new"]
      ],
      [
          "navProjects",
          [":private_router/route", "/projects"]
      ],
      [
          "navOrganizations",
          [":private_router/route", "/organizations"]
      ],
      [
          "navBillings",
          [":private_router/route", "/billings"]
      ],
      [
          "navVersions",
          [":private_router/route", "/versions"]
      ],
      [
          "accountSettings",
          [":private_router/route", "/settings/account"]
      ],
      [
          "profileSettings",
          [":private_router/route", "/settings/profile"]
      ],
      [
          "navUserProfile",
          [":private_router/route", "/user"]
      ],
      [
          "toggleShortcuts",
          [":nav_main_private_layout/state", "toggleDirection"]
      ],
      [
          "hideShortcuts",
          [":nav_main_private_layout/state", "hideDirection"]
      ],
      [
          "navSignout",
          [">login/flow", "logoutUser"]
      ],
      [
          "navBlog",
          [":common_router/route", "/blog"]
      ],
      [
          "navDocs",
          [":common_router/route", "/docs"]
      ],
      [
          "navHelp",
          [":common_router/route", "/support"]
      ],
      [
          "toggleMobileMenu",
          [":state", "toggleMobileMenu"]
      ]
    ],
    "markup": [
      "/root/private/nav_main_private.html"
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "nav_main_private_layout"
}
