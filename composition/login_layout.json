{
  "client": {
    "config": {
      "title": "Login",
      "templates": {
        "layout": {
          "to": ".pages-container",
          "html": "/public/login.html"
        }
      },
      "defaultTemplate": "layout"
    },
    "markup": [
      "/public/login.html"
    ],
    "flow": [
      [
        "itemClick",
        [
          ":ALTR",
          {
            "data": {
              "url": "{event.target.href}"
            }
          }
        ],
        ":public_router/route"
      ],
      [
          "toggleMobileMenu",
          [":state", "toggleMobileMenu"]
      ],
      [
          "publicHome",
          [":public_router/route", "/"]
      ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "login_layout"
}
