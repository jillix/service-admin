{
  "client": {
    "config": {
      "title": "Infrastructure",
      "templates": {
        "layout": {
          "to": ".gcp-infrastructure",
          "html": "/private/infrastructure/infrastructure_list.html"
        }
      },
      "defaultTemplate": "layout",
      "states": {
          "toggleAdvOptions": [
              {
                  "sel": ".option span",
                  "toggle": ["hide"]
              },
              {
                  "sel": ".adv-container",
                  "toggle": ["hide"]
              }
          ]
      },
      "domEvents": [
          {
              "on": "click",
              "selector": ".option",
              "flow": "toggleArrow"
          }
      ]
    },
    "markup": [
      "/private/infrastructure/infrastructure_list.html"
    ],
    "flow": [
      [
          "toggleArrow",
          [":state", "toggleAdvOptions"]
      ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "gcp_infrastructure_layout"
}
