{
  "client": {
    "config": {
      "title": "Money",
      "templates": {
        "layout": {
          "to": ".pages-container",
          "html": "/private/money_options.html"
        }
      },
      "defaultTemplate": "layout",
      "states": {
          "hide-all": [{
              "sel": ".money-options > div",
              "add": ["hide"]
          }],
          "invoices": [{
              "sel": ".money-options > .invoices",
              "rm": ["hide"]
          }],
          "history": [{
              "sel": ".money-options > .history",
              "rm": ["hide"]
          }],
          "credit_card": [{
              "sel": ".money-options > .credit-card",
              "rm": ["hide"]
          }],
          "vouchers": [{
              "sel": ".money-options > .vouchers",
              "rm": ["hide"]
          }],
          "earnings": [{
              "sel": ".money-options > .earnings",
              "rm": ["hide"]
          }]
      }
    },
    "markup": [
      "/private/money_options.html"
    ],
    "flow": [
        [
            "renderedDOM",
            "nav_money_layout/render"
        ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "money_layout"
}
