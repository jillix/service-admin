{
  "client": {
    "config": {
      "templates": {
        "layout": {
          "to": ".main-nav",
          "html": "/private/nav_main_private.html",
          "render": true
        }
      },
      "defaultTemplate": "layout",
      "states": {
        "toggleMobileMenu": [{
            "sel": ".first-col",
            "toggle": ["show"]
        },
        {
            "sel": ".collapsable-nav-1",
            "toggle": ["show"]
        },
        {
            "sel": ".second-col",
            "toggle": ["show"]
        },
        {
            "sel": ".collapsable-nav-2",
            "toggle": ["show"]
        }
        ]
      },
      "domEvents": [
        {
          "on": "click",
          "element": "documentation",
          "flow": "documentation"
        },
        {
          "on": "click",
          "element": "tutorials",
          "flow": "support"
        },
        {
          "on": "click",
          "element": "contact",
          "flow": "contact"
        },
        {
          "on": "click",
          "element": "about",
          "flow": "about"
        },
        {
          "on": "click",
          "element": "blog",
          "flow": "blog"
        },
        {
          "on": "click",
          "element": "terms",
          "flow": "terms"
        },
        {
          "on": "click",
          "element": "privacy",
          "flow": "privacy"
        },
        {
          "on": "click",
          "element": "signout",
          "flow": "navSignout"
        },
        {
            "on": "click",
            "selector": ".menu-icon",
            "flow": "toggleMobileMenu"
        },
        {
          "on": "click",
          "element": "jillix-logo",
          "flow": "privateHome"
        }
      ]
    },
    "flow": [
      [
          "privateHome",
          [":private_router/route", "/"]
      ],
      [
          "navApps",
          [":private_router/route", "/apps"]
      ],
      [
          "navStores",
          [":private_router/route", "/stores"]
      ],
      [
          "navDomains",
          [":private_router/route", "/domains"]
      ],
      [
          "navUserSettings",
          [":private_router/route", "/settings/user/general-info"]
      ],
      [
          "navUserProfile",
          [":private_router/route", "/user"]
      ],
      [
          "toggleShortcuts",
          [":nav_main_private_layout/state", "toggleDirection"]
      ],
      [
          "hideShortcuts",
          [":nav_main_private_layout/state", "hideDirection"]
      ],
      [
          "navSignout",
          [">login/flow", "logoutUser"]
      ],
      [
          "toggleMobileMenu",
          [":state", "toggleMobileMenu"]
      ]
    ],
    "markup": [
      "/private/nav_main_private.html"
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "nav_main_private_layout"
}
