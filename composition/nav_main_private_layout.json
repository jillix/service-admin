{
  "client": {
    "config": {
      "templates": {
        "layout": {
          "to": ".main-nav",
          "html": "/private/nav_main_private.html",
          "render": true
        }
      },
      "defaultTemplate": "layout",
      "states": {
          "resetNav": [
              {
                  "sel": ".active-visible li",
                  "add": [
                      "hide"
                  ]
              }
          ],
          "activeDashboard": [
              {
                  "states": [
                      "resetNav"
                  ],
                  "sel": ".active-visible .dashboard",
                  "rm": [
                      "hide"
                  ]
              }
          ],
          "nav": [
              {
                  "states": [
                      "resetNav"
                  ],
                  "rm": [
                      "hide"
                  ]
              }
          ],
          "showNavigationMenu": [
              {
                  "sel": ".navigation-menu-hidden",
                  "rm": ["hide"]
              },
              {
                  "sel": ".active-visible",
                  "add": ["hide"]
              }
          ],
          "hideNavigationMenu": [
              {
                  "sel": ".navigation-menu-hidden",
                  "add": ["hide"]
              },
              {
                  "sel": ".active-visible",
                  "rm": ["hide"]
              }
          ],
          "toggleDirection": [
              {
                  "sel": ".direction-toggle",
                  "toggle": [
                      "hide"
                  ]
              },
              {
                  "sel": ".direction-arrow",
                  "toggle": [
                      "hide"
                  ]
              }
          ],
          "hideDirection": [
              {
                  "sel": ".direction-toggle",
                  "add": [
                      "hide"
                  ]
              },
              {
                  "sel": ".direction-arrow",
                  "add": [
                      "hide"
                  ]
              }
          ],
          "toggleMobileMenu": [{
              "sel": ".first-col",
              "toggle": ["show"]
          },
          {
              "sel": ".collapsable-nav-1",
              "toggle": ["show"]
          },
          {
              "sel": ".second-col",
              "toggle": ["show"]
          },
          {
              "sel": ".collapsable-nav-2",
              "toggle": ["show"]
          }
          ]
      },
      "domEvents": [
        {
          "on": "mouseover",
          "selector": ".navigation-menu",
          "flow": "showNavigationMenu"
        },
        {
            "on": "mouseleave",
            "selector": ".navigation-menu",
            "flow": "hideNavigationMenu"
        },
        {
          "on": "click",
          "element": "admin-logo",
          "flow": "privateHome"
        },
        {
          "on": "click",
          "element": "money",
          "flow": "money"
        },
        {
          "on": "click",
          "element": "infrastructure",
          "flow": "infrastructure"
        },
        {
          "on": "click",
          "element": "user-management",
          "flow": "user-management"
        },
        {
          "on": "click",
          "element": "dashboard",
          "flow": "dashboard"
        },
        {
          "on": "click",
          "element": "signout",
          "flow": "navSignout"
        },
        {
            "on": "click",
            "selector": ".menu-icon",
            "flow": "toggleMobileMenu"
        },
        {
          "on": "click",
          "element": "jillix-logo",
          "flow": "privateHome"
        }
      ]
    },
    "flow": [
      [
          "showNavigationMenu",
          [":state", "showNavigationMenu"]
      ],
      [
          "hideNavigationMenu",
          [":state", "hideNavigationMenu"]
      ],
      [
          "privateHome",
          [":private_router/route", "/"]
      ],
      [
          "money",
          [":private_router/route", "/money"],
          [":state", "hideNavigationMenu"]
      ],
      [
          "infrastructure",
          [":private_router/route", "/infrastructure"],
          [":state", "hideNavigationMenu"]

      ],
      [
          "user-management",
          [":private_router/route", "/user-management"],
          [":state", "hideNavigationMenu"]
      ],
      [
          "dashboard",
          [":private_router/route", "/dashboard"],
          [":state", "hideNavigationMenu"]
      ],
      [
          "navSignout",
          [">login/flow", "logoutUser"]
      ],
      [
          "toggleMobileMenu",
          [":state", "toggleMobileMenu"]
      ]
    ],
    "markup": [
      "/private/nav_main_private.html"
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "nav_main_private_layout"
}
