{
    "client": {
        "config": {
            "routes": [
                "homepage",
                {
                    "money": [
                        "money",
                        {
                            "invoices": "invoices",
                            "history": "history",
                            "credit-cards": "credit-cards",
                            "vouchers": "vouchers",
                            "earnings": "earnings"
                        }
                    ],
                    "infrastructure": "infrastructure",
                    "user-management": [
                        "user-management",
                        {
                            "users": "users",
                            "organizations": "organizations"
                        }
                    ],
                    "dashboard": "dashboard"
                }
            ],
            "customData": {
                "clear": true
            }
        },
        "flow": [
            [
                "add_active_class",
                [
                    ":ALTR",
                    {
                        "data": {
                            "selector": ".navigation-menu [data-element='{_path.0}']"
                        }
                    }
                ],
                [":nav_main_private_layout/state", "nav"]
            ],
            [
                "homepage",
                "dashboard_layout/render",
                [":nav_main_private_layout/state", "activeDashboard"]
            ],
            [
                "dashboard",
                "dashboard_layout/render",
                ["flow", "add_active_class"]

            ],
            [
                "money",
                "money_options_layout/render",
                ["flow", "add_active_class"]

            ],
            [
                "invoices",
                "invoices_layout/render",
                [":money_options_layout/state", "invoices"],
                ["flow", "add_active_class"]
            ],
            [
                "infrastructure",
                "infrastructure_layout/render",
                ["flow", "add_active_class"]
            ],
            [
                "user-management",
                "user_management_layout/render",
                [":user_management_layout/state", "displayUsers"],
                ["flow", "add_active_class"]
            ],
            [
                "users",
                "user_management_layout/render",
                [":user_management_layout/state", "displayUsers"],
                ["flow", "add_active_class"]
            ],
            [
                "organizations",
                "user_management_layout/render",
                [":user_management_layout/state", "displayOrganizations"],
                ["flow", "add_active_class"]
            ]

        ]
    },
    "roles": {
        "*": true
    },
    "module": "engine-ruut",
    "name": "private_router"
}
