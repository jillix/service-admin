{
  "client": {
    "config": {
      "title": "Infrastructure",
      "templates": {
        "layout": {
          "to": ".pages-container",
          "html": "/private/infrastructure/infrastructure.html",
          "render": true
        }
      },
      "defaultTemplate": "layout",
      "states": {

          "resetContent": [
              {
                  "sel": ".infrastructure-content > div",
                  "add": ["hide"]
              }
          ],
          "displayVersionManagement": [
              {
                  "states": ["resetContent"],
                  "sel": ".version-management",
                  "rm": ["hide"]
              }
          ],
          "displayGcpInfrastructure": [
              {
                  "states": ["resetContent"],
                  "sel": ".gcp-infrastructure",
                  "rm": ["hide"]
              }
          ],
          "displayJillixInfrastructure": [
              {
                  "states": ["resetContent"],
                  "sel": ".jillix-infrastructure",
                  "rm": ["hide"]
              }
         ],
          "displayNewEngineVersion": [
              {
                  "states": ["resetContent"],
                  "sel": ".new-engine-version",
                  "rm": ["hide"]
              }
          ]
      }
    },
    "markup": [
      "/private/infrastructure/infrastructure.html"
    ],
    "flow": [
        [
            "renderedDOM",
            "infrastructure_nav_layout/render",
            "version_management_layout/render",
            "gcp_infrastructure_layout/render",
            "new_engine_version_layout/render",
            "jillix_infrastructure_layout/render"
        ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "infrastructure_layout"
}
