{
  "client": {
    "config": {
      "title": "User management",
      "templates": {
        "layout": {
          "to": ".pages-container",
          "html": "/private/user-management/user_management.html",
          "render": true
        }
      },
      "defaultTemplate": "layout",
      "states": {

          "resetContent": [
              {
                  "sel": ".user-management-content > div",
                  "add": ["hide"]
              }
          ],
          "displayUsers": [
              {
                  "states": ["resetContent"],
                  "sel": ".users",
                  "rm": ["hide"]
              }
          ],
          "displayOrganizations": [
              {
                  "states": ["resetContent"],
                  "sel": ".organizations",
                  "rm": ["hide"]
              }
          ],
          "displayItem": [
              {
                  "states": ["resetContent"],
                  "sel": ".specific-item",
                  "rm": ["hide"]
              }
          ]
      }
    },
    "markup": [
      "/private/user-management/user_management.html"
    ],
    "flow": [
        [
            "renderedDOM",
            "user_management_nav_layout/render",
            "users_layout/render",
            "organizations_layout/render"
        ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "user_management_layout"
}
