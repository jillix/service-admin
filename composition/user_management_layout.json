{
  "client": {
    "config": {
      "title": "User management",
      "templates": {
        "layout": {
          "to": ".pages-container",
          "html": "/private/user-management/user_management.html",
          "render": true
        }
      },
      "defaultTemplate": "layout",
      "states": {
          "displayUsers": [{
              "sel": ".users",
              "rm": ["hide"]
          },
          {
              "sel": ".organizations",
              "add": ["hide"]
          }
          ],
          "displayOrganizations": [{
              "sel": ".users",
              "add": ["hide"]
          },
          {
              "sel": ".organizations",
              "rm": ["hide"]
          }
          ]
      }
    },
    "markup": [
      "/private/user-management/user_management.html"
    ],
    "flow": [
        [
            "renderedDOM",
            "user_management_nav_layout/render"
        ]
    ]
  },
  "roles": {
    "*": true
  },
  "module": "view",
  "name": "user_management_layout"
}
